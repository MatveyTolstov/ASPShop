@{
    ViewData["Title"] = "SignUp";
}
@model ShopHunter.Models.BDConn.Users

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="Css.css">
</head>
<body>
<header>

    <div class="logo">
        <h2>Hunter&Fish</h2>
    </div>
    <nav class="navbar">
        <ul class="nav-list">
            <li>
                <a href="#New">Новинки</a>
            </li>
            <li>
                <a href=@Url.Action("Shop")>Магазин</a>
            </li>
            <li>
                <a href="#Contacts">Контакты</a>
            </li>
            <li>
                <a href="#About">О Нас</a>
            </li>
        </ul>
    </nav>
    <div class="shop-icon">
        <img src="data:image/png;base64,"/>
        <img src="data:image/png;base64,"/>
    </div>

</header>
<div class="imageForest">
    <form class="blockRegister" asp-antiforgery="true" asp-action="SignUp" asp-controller="Home">
        <h2>Register</h2>
        <p>Если у вас есть аккаунт </p>
        <small>Нету аккаунта? <a asp-action="SignUp" asp-controller="Home">Регистрация</a></small>
        <h4>Login</h4>
        <div>
            <input asp-for="@Model.Login" name="Login" minlength="2" maxlength="50" autofocus="" placeholder="login"/>
        </div>
        <h4>Password</h4>
        <div>
            <input asp-for="@Model.Password" name="Password" value="" placeholder="password" type="password"/>
        </div>
        <div>
            <button>Register</button>
        </div>
    </form>
</div>
<footer >
    <div class="footer-content">
        <nav>
            <ul class="footer-nav">
                <li>
                    <a href="#">Политика конфиденциальности</a>
                </li>
                <li>
                    <a href="#">Условия использования</a>
                </li>
                <li>
                    <a href="#">Контакты</a>
                </li>
            </ul>
        </nav>
        <p>© 2024 Hunter&Fish. Все права защищены.</p>
    </div>
</footer>
</body>
<script src="functions.js"></script>
</html>